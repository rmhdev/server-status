{
  "name": "rmhdev/server-status",
  "description": "Server status app",
  "license": "mit",
  "type": "project",
  "authors": [
    {
      "name": "Roberto Mart√≠n H",
      "homepage": "https://github.com/rmhdev",
      "email": "rmh.dev@gmail.com",
      "role": "Project Founder"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-iconv": "*",
    "league/uri": "^5.2",
    "php-http/client-implementation": "^1.0",
    "php-http/guzzle6-adapter": "^1.1",
    "ramsey/uuid": "^3.7",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/asset": "^4.0",
    "symfony/console": "^4.0",
    "symfony/expression-language": "^4.0",
    "symfony/flex": "^1.0",
    "symfony/framework-bundle": "^4.0",
    "symfony/lts": "^4@dev",
    "symfony/monolog-bundle": "^3.1",
    "symfony/orm-pack": "^1.0",
    "symfony/security-bundle": "^4.0",
    "symfony/stopwatch": "^4.0",
    "symfony/translation": "^4.0",
    "symfony/twig-bundle": "^4.0",
    "symfony/yaml": "^4.0",
    "twig/extensions": "^1.5"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "pdepend/pdepend": "^2.5",
    "php-http/mock-client": "^1.0",
    "phpmd/phpmd": "^2.6",
    "squizlabs/php_codesniffer": "^3.1",
    "symfony/browser-kit": "^4.0",
    "symfony/css-selector": "^4.0",
    "symfony/dotenv": "^4.0",
    "symfony/phpunit-bridge": "^4.0",
    "symfony/profiler-pack": "^1.0",
    "symfony/web-server-bundle": "^4.0",
    "zendframework/zend-diactoros": "^1.7"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "ServerStatus\\": "src/ServerStatus/",
      "App\\": "src/App/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ServerStatus\\Tests\\": "tests/ServerStatus/",
      "App\\Tests\\": "tests/App/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01C713FXYDZC1NFC6C1D9JGKGD",
      "allow-contrib": false
    }
  }
}
