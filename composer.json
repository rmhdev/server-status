{
  "name": "rmhdev/server-status",
  "description": "Server status app",
  "license": "mit",
  "type": "project",
  "authors": [
    {
      "name": "Roberto Mart√≠n H",
      "homepage": "https://github.com/rmhdev",
      "email": "rmh.dev@gmail.com",
      "role": "Project Founder"
    }
  ],
  "require": {
    "php": "^7.1.0",
    "league/uri": "^5.2",
    "ramsey/uuid": "^3.7",
    "php-http/client-implementation": "^1.0"
  },
  "require-dev": {
    "phpmd/phpmd": "^2.6",
    "phpunit/phpunit": "^7.0",
    "squizlabs/php_codesniffer": "^3.1",
    "pdepend/pdepend" : "^2.5",
    "php-http/mock-client": "^1.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "ServerStatus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ServerStatus\\Tests\\": "tests/"
    }
  }
}
